<div class="modal" id="tabulacao">
    <div class="modal-dialog">
<div class="modal-content">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title">Adicionar nova tabulação</h4>
    </div>
    <div class="modal-body">
        <form class="form-horizontal">
            <div class="well text-center">
                <strong>Inserir novo registro de tabulação</strong>
            </div>
            <div class="well text-center">
                <fieldset>
                    <form class="form-horizontal">
                        <fieldset>
                            <div class="form-group">
                                <label for="inputEmail" class="col-lg-3 control-label">Titulo</label>
                                <div class="col-lg-9">
                                    <input type="text" class="form-control" id="#" placeholder="Receptivo-Pesquisa-001">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="textArea" class="col-lg-3 control-label">Observação</label>
                                <div class="col-lg-9">
                                    <textarea class="form-control" rows="3" id="textArea" placeholder="Tabulação formulário de entrada criado para pesquisas acompanhar fluxo de pesquisa e vendas Renault."></textarea>
                                    <span class="help-block">Informe uma observação sobre a criação deste auxiliar de marca.</span>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="select" class="col-lg-3 control-label">Atribuições</label>
                                <div class="col-lg-9">
                                    <select multiple="" class="form-control">
                                        <option>1 - Formulário de satisfação produto</option>
                                        <option>2 - Formulário de satisfação entrega </option>
                                        <option>3 - Formulário de satisfação cliente</option>
                                        <option>4 - Formulário de satisfação atendimento</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="select" class="col-lg-3 control-label">Departamento</label>
                                <div class="col-lg-9">
                                    <select class="form-control input-sm" id="select">
                                        <option>1</option>
                                        <option>2</option>
                                        <option>3</option>
                                        <option>4</option>
                                        <option>5</option>
                                    </select>
                                    <br>
                                </div>
                            </div>
                        </fieldset>
                    </form>
                </fieldset>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default btn-sm" data-dismiss="modal">Cancelar</button>
        <button type="button" class="btn btn-info btn-sm">Salvar alterações</button>
    </div>
</div>
    </div>
</div>

<div class="modal" id="departamento">
    <div class="modal-dialog">
<div class="modal-content">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title">Adicionar nova entidade</h4>
    </div>
    <div class="modal-body">
        <form class="form-horizontal">
            <div class="well text-center">
                <strong>Inserir novo registro de entidade</strong>
            </div>
            <div class="well text-center">
                <fieldset>
                    <form class="form-horizontal">
                        <fieldset>
                            <div class="form-group">
                                <label class="col-lg-3 control-label" for="inputSmall">Nome</label>
                                <div class="col-lg-9">
                                    <input class="form-control input-sm" type="text" id="inputSmall" placeholder="Receptivo-001">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="textArea" class="col-lg-3 control-label">Descrição</label>
                                <div class="col-lg-9">
                                    <textarea class="form-control" rows="3" id="textArea" placeholder="Equipe do receptivo de vendas. bloco 01"></textarea>
                                    <span class="help-block">Informe uma breve descrição que defina a entidade</span>
                                </div>
                            </div>
                        </fieldset>
                    </form>
                </fieldset>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default btn-sm" data-dismiss="modal">Cancelar</button>
        <button type="button" class="btn btn-info btn-sm">Salvar alterações</button>
    </div>
</div>
    </div>
</div>

